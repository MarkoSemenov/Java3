<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="ChangeNickController"
            prefHeight="200.0" prefWidth="400.0">


    <VBox alignment="CENTER" layoutX="100.0" layoutY="65.0" AnchorPane.leftAnchor="100.0" AnchorPane.topAnchor="65.0">
        <TextField fx:id="nickname" prefHeight="30" prefWidth="200" promptText="new Nickname" onAction="#changeNickname" />
        <Button prefHeight="30" prefWidth="200" text="Ok" onAction="#changeNickname" />
    </VBox>


</AnchorPane>
